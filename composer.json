{
    "name": "broken-titan/laravel-klaviyo-channel",
    "description": "Klaviyo notification channel for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marshall Miller",
            "email": "contact@marshall-miller.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "klaviyo/php-sdk": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrokenTitan\\Klaviyo\\Providers\\KlaviyoServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "BrokenTitan\\Klaviyo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor\\bin\\phpunit"
        ]
    }
}
